clear all
fs = 400;
fc = 20;
fnorm = fc*2/fs;
dt = 1/fs;
t1 = 0.25;
t0 = 0;
t = t0:dt:t1;
num_samples = (t1-t0)/dt;
pnoise = [0.1*wgn(num_samples,1,0)][zeros(num_samples)];
onoise = [0.1*wgn(num_samples,1,0) zeros(num_samples)];
[b,a] = butter(2,fnorm);
sim('butterworth_mdl.mdl')
y = padarray(y,1,'pre')
plot(t,y,t,u)